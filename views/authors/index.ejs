<h1>Authors List</h1>

<div class="container">
  <div class="row">
    <% authors.forEach((author)=>{%>
        <div class="col-md-4">
            <div class="card mb-3">
            <div class="card-body">
                <h5 class="card-title"><a href="/authors/<%=author.slug %>"><%= author.name %></a></h5>
                <!-- You can add more information about the author here if needed -->
            </div>
            </div>
        </div>
    <%}) %>
      
  </div>
</div>
